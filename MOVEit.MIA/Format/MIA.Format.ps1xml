      </TableControl>
    </View>
    <View>
      <Name>MIACustomScript</Name>
      <ViewSelectedBy>
        <TypeName>MIACustomScript</TypeName>
      </ViewSelectedBy>
      <TableControl>
        <TableHeaders>
          <TableColumnHeader>
            <Label>Id</Label>
            <Width>12</Width>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Name</Label>
            <Width>48</Width>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Description</Label>
            <Width>60</Width>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Lang</Label>
            <Width>11</Width>
          </TableColumnHeader>
        </TableHeaders>
        <TableRowEntries>
          <TableRowEntry>
            <TableColumnItems>
              <TableColumnItem>
                <PropertyName>id</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>Name</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>Description</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>Lang</PropertyName>
              </TableColumnItem>
            </TableColumnItems>
          </TableRowEntry>
        </TableRowEntries>
      </TableControl>
    </View>
    <View>
      <Name>MIAPaging</Name>
      <OutOfBand />        
      <ViewSelectedBy>
        <TypeName>MIAPaging</TypeName>
      </ViewSelectedBy>
      <CustomControl>     
        <CustomEntries>
          <CustomEntry>                              
            <CustomItem>
              <NewLine/>
              <Text>Total items: </Text>
              <ExpressionBinding>
                <PropertyName>totalItems</PropertyName>
              </ExpressionBinding>   
              <Text>  Per page: </Text>
              <ExpressionBinding>
                <PropertyName>perPage</PropertyName>
              </ExpressionBinding>     
              <Text>  Displaying page: </Text>
              <ExpressionBinding>
                <PropertyName>page</PropertyName>
              </ExpressionBinding> 
              <Text> of </Text>
              <ExpressionBinding>
                <PropertyName>totalPages</PropertyName>
              </ExpressionBinding>                          
            </CustomItem>               
          </CustomEntry>
        </CustomEntries>       
      </CustomControl>
    </View>
  </ViewDefinitions>
</Configuration>